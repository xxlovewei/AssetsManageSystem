<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.MnUrlMetricEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.MnUrlMetric">
        <id column="NODE" property="node" />
        <result column="NAME" property="name" />
        <result column="IS_RUNNING" property="isRunning" />
        <result column="URL" property="url" />
        <result column="STATUS" property="status" />
        <result column="DR" property="dr" />
        <result column="INTERVAL_TIME" property="intervalTime" />
        <result column="CURCNT" property="curcnt" />
        <result column="MARK" property="mark" />
        <result column="THRESHOLD" property="threshold" />
        <result column="MAXWARN" property="maxwarn" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        NODE, NAME, IS_RUNNING, URL, STATUS, DR, INTERVAL_TIME, CURCNT, MARK, THRESHOLD, MAXWARN
    </sql>

</mapper>
