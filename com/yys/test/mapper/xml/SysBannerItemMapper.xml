<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.SysBannerItemEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.SysBannerItem">
        <id column="ID" property="id" />
        <result column="BANNER_ID" property="bannerId" />
        <result column="TYPE" property="type" />
        <result column="NAME" property="name" />
        <result column="PIC_ID" property="picId" />
        <result column="CT" property="ct" />
        <result column="RK" property="rk" />
        <result column="IS_USED" property="isUsed" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, BANNER_ID, TYPE, NAME, PIC_ID, CT, RK, IS_USED
    </sql>

</mapper>
