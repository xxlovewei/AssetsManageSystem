<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.CtCategoryEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.CtCategory">
        <id column="ID" property="id" />
        <result column="ROOT" property="root" />
        <result column="NAME" property="name" />
        <result column="MPIC" property="mpic" />
        <result column="PARENT_ID" property="parentId" />
        <result column="ROUTE" property="route" />
        <result column="MARK" property="mark" />
        <result column="NODE_LEVEL" property="nodeLevel" />
        <result column="OD" property="od" />
        <result column="ISACTION" property="isaction" />
        <result column="DELETED" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, ROOT, NAME, MPIC, PARENT_ID, ROUTE, MARK, NODE_LEVEL, OD, ISACTION, DELETED
    </sql>

</mapper>
