<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.ProductCategoryEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.ProductCategory">
        <id column="ID" property="id" />
        <result column="PARENT_ID" property="parentId" />
        <result column="TEXT" property="text" />
        <result column="IS_DELETED" property="isDeleted" />
        <result column="IS_CAT" property="isCat" />
        <result column="ROUTE" property="route" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, PARENT_ID, TEXT, IS_DELETED, IS_CAT, ROUTE
    </sql>

</mapper>
