        <result column="PRODUCETIME" property="producetime" />
        <result column="INSERTTIME" property="inserttime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        NODE, ORANAME, STATUS, WAITING_SESSION, USERNAME, OBJECT_NAME, OBJECT_ID, OWNER, OBJECT_TYPE, COMMAND, SID, PRODUCETIME, INSERTTIME
