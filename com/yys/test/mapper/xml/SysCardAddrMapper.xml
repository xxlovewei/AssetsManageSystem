<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.SysCardAddrEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.SysCardAddr">
        <id column="ID" property="id" />
        <result column="DR" property="dr" />
        <result column="SHENGF" property="shengf" />
        <result column="CHENGS" property="chengs" />
        <result column="QUX" property="qux" />
        <result column="SHENGFMC" property="shengfmc" />
        <result column="CHENGSMC" property="chengsmc" />
        <result column="QUXMC" property="quxmc" />
        <result column="USER_ID" property="userId" />
        <result column="CARD_NO" property="cardNo" />
        <result column="CARD_BANK" property="cardBank" />
        <result column="CARD_PART" property="cardPart" />
        <result column="CARD_NAME" property="cardName" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, DR, SHENGF, CHENGS, QUX, SHENGFMC, CHENGSMC, QUXMC, USER_ID, CARD_NO, CARD_BANK, CARD_PART, CARD_NAME
    </sql>

</mapper>
