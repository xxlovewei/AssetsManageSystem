    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.MnOradbSize">
        <result column="NODE" property="node" />
        <result column="ORANAME" property="oraname" />
        <result column="ALLOCATE" property="allocate" />
        <result column="FREE" property="free" />
        <result column="PRODUCETIME" property="producetime" />
        <result column="INSERTTIME" property="inserttime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        NODE, ORANAME, ALLOCATE, FREE, PRODUCETIME, INSERTTIME
    </sql>

</mapper>
