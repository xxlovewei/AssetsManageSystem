<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.SysSessionEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.SysSession">
        <id column="COOKIE" property="cookie" />
        <result column="TOKEN" property="token" />
        <result column="LASTACCESS" property="lastaccess" />
        <result column="EXPIRE" property="expire" />
        <result column="DTSESSION" property="dtsession" />
        <result column="ID" property="id" />
        <result column="USER_ID" property="userId" />
        <result column="START_TIME" property="startTime" />
        <result column="LOGIN_TIME" property="loginTime" />
        <result column="IP" property="ip" />
        <result column="AGENT" property="agent" />
        <result column="CLIENT" property="client" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        TOKEN, LASTACCESS, EXPIRE, COOKIE, DTSESSION, ID, USER_ID, START_TIME, LOGIN_TIME, IP, AGENT, CLIENT
    </sql>

</mapper>
