<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.MnNginxStatusEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.MnNginxStatus">
        <result column="NODE" property="node" />
        <result column="ACTIVE" property="active" />
        <result column="READING" property="reading" />
        <result column="WRITING" property="writing" />
        <result column="WAITING" property="waiting" />
        <result column="ACCEPTS" property="accepts" />
        <result column="HANDLED" property="handled" />
        <result column="REQUESTS" property="requests" />
        <result column="PRODUCETIME" property="producetime" />
        <result column="INSERTTIME" property="inserttime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        NODE, ACTIVE, READING, WRITING, WAITING, ACCEPTS, HANDLED, REQUESTS, PRODUCETIME, INSERTTIME
    </sql>

</mapper>
