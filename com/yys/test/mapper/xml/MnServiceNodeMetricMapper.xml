<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.MnServiceNodeMetricEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.MnServiceNodeMetric">
        <id column="ID" property="id" />
        <result column="SERVICE_ID" property="serviceId" />
        <result column="NODE_ID" property="nodeId" />
        <result column="METRIC_ID" property="metricId" />
        <result column="DATA_INTERVAL" property="dataInterval" />
        <result column="IS_SHOW" property="isShow" />
        <result column="MTYPE" property="mtype" />
        <result column="V_A_V" property="vAV" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, SERVICE_ID, NODE_ID, METRIC_ID, DATA_INTERVAL, IS_SHOW, MTYPE, V_A_V
    </sql>

</mapper>
