<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.SysModulesEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.SysModules">
        <id column="MODULE_ID" property="moduleId" />
        <result column="MODULE_ROOTID" property="moduleRootid" />
        <result column="KEY" property="key" />
        <result column="IS_ACTION" property="isAction" />
        <result column="TYPE" property="type" />
        <result column="MENU_ID" property="menuId" />
        <result column="ORG_ID" property="orgId" />
        <result column="MARK" property="mark" />
        <result column="MODULE_NAME" property="moduleName" />
        <result column="DELETED" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        MODULE_ID, MODULE_ROOTID, KEY, IS_ACTION, TYPE, MENU_ID, ORG_ID, MARK, MODULE_NAME, DELETED
    </sql>

</mapper>
