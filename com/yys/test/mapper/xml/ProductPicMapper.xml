<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.ProductPicEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.ProductPic">
        <id column="ID" property="id" />
        <result column="SPU" property="spu" />
        <result column="PIC_ID" property="picId" />
        <result column="TYPE" property="type" />
        <result column="OD" property="od" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, SPU, PIC_ID, TYPE, OD
    </sql>

</mapper>
