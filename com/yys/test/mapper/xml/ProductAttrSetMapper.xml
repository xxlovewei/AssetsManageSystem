<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yys.test.mapper.ProductAttrSetEntity">

    <!-- 开启二级缓存 -->
    <cache type="org.mybatis.caches.ehcache.LoggingEhcache"/>

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yys.test.entity.ProductAttrSet">
        <id column="ID" property="id" />
        <result column="SPU" property="spu" />
        <result column="ATTR_SET_ID" property="attrSetId" />
        <result column="ATTR_ID" property="attrId" />
        <result column="ATTR_NAME" property="attrName" />
        <result column="IS_SKU" property="isSku" />
        <result column="VALUE" property="value" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        ID, SPU, ATTR_SET_ID, ATTR_ID, ATTR_NAME, IS_SKU, VALUE
    </sql>

</mapper>
